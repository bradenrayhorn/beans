{
  "name": "beans",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint --max-warnings 0 src",
    "format": "prettier --write '{e2e,src}/**/*.{ts,tsx,js,jsx}'",
    "format:lint": "prettier --check '{e2e,src}/**/*.{ts,tsx,js,jsx}'",
    "build:e2e": "tsc && vite build --mode=development",
    "preview": "vite preview",
    "e2e": "playwright test",
    "e2e:debug": "playwright test --debug",
    "e2e:ci": "WAIT_ON_TIMEOUT=10000 start-server-and-test backend:start http-get://127.0.0.1:8000/health-check",
    "backend:start": "(cd ../ && make run)",
    "test": "vitest",
    "test:ci": "vitest run"
  },
  "dependencies": {
    "@chakra-ui/icons": "2.0.17",
    "@chakra-ui/react": "2.5.1",
    "@chakra-ui/theme-tools": "2.0.16",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@tanstack/react-query": "4.26.1",
    "dayjs": "1.11.7",
    "downshift": "7.3.0",
    "fraction.js": "4.2.0",
    "framer-motion": "10.1.0",
    "ky": "0.33.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-focus-lock": "^2.9.4",
    "react-hook-form": "7.43.5",
    "react-number-format": "5.1.4",
    "react-router-dom": "6.8.2",
    "zustand": "4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "1.31.2",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "3.1.0",
    "cross-fetch": "3.1.5",
    "eslint": "8.35.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "jsdom": "21.1.0",
    "msw": "1.1.0",
    "prettier": "2.8.4",
    "start-server-and-test": "2.0.0",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vitest": "0.29.2"
  }
}
