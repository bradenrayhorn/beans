{
  "name": "beans",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint --max-warnings 0 src",
    "format": "prettier --write '{e2e,src}/**/*.{ts,tsx,js,jsx}'",
    "format:lint": "prettier --check '{e2e,src}/**/*.{ts,tsx,js,jsx}'",
    "build:e2e": "tsc && vite build --mode=development",
    "preview": "vite preview",
    "e2e": "playwright test",
    "e2e:debug": "playwright test --debug",
    "e2e:ci": "WAIT_ON_TIMEOUT=10000 start-server-and-test backend:start http-get://127.0.0.1:8000/health-check",
    "backend:start": "(cd ../ && make run)",
    "test": "vitest",
    "test:ci": "vitest run"
  },
  "dependencies": {
    "@chakra-ui/icons": "2.1.0",
    "@chakra-ui/react": "2.8.0",
    "@chakra-ui/theme-tools": "2.1.0",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@tanstack/react-query": "4.32.1",
    "dayjs": "1.11.9",
    "downshift": "7.6.2",
    "fraction.js": "4.2.0",
    "framer-motion": "10.15.0",
    "ky": "0.33.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-focus-lock": "^2.9.4",
    "react-hook-form": "7.45.2",
    "react-number-format": "5.2.2",
    "react-router-dom": "6.14.2",
    "zustand": "4.4.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@playwright/test": "1.36.2",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "4.0.4",
    "babel-preset-react-app": "^10.0.1",
    "confusing-browser-globals": "^1.0.11",
    "cross-fetch": "4.0.0",
    "eslint": "8.46.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "22.1.0",
    "msw": "1.2.3",
    "prettier": "2.8.8",
    "start-server-and-test": "2.0.0",
    "typescript": "5.1.6",
    "vite": "4.4.7",
    "vitest": "0.33.0"
  }
}
